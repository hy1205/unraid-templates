<?xml version="1.0"?>
<Container version="2">
  <Name>trackarr</Name>
  <Repository>hotio/trackarr</Repository>
  <Branch>
    <Tag>stable</Tag>
    <TagDescription>Stable version</TagDescription>
  </Branch>
  <Branch>
    <Tag>unstable</Tag>
    <TagDescription>Unstable version, -dev release</TagDescription>
  </Branch>
  <Registry>https://hub.docker.com/r/hotio/trackarr</Registry>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://discord.gg/3SnkuKp</Support>
  <Project>https://github.com/hotio/docker-trackarr</Project>
  <Category>Downloaders:</Category>
  <TemplateURL>https://raw.githubusercontent.com/hotio/unraid-templates/master/hotio/trackarr.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/hotio/unraid-templates/master/hotio/img/trackarr.png</Icon>
  <Overview>Trackarr monitors tracker announce IRC channel, parses the announcements, and forwards those announcements to ARR PVRs (eg Sonarr).</Overview>
  <Description>Trackarr monitors tracker announce IRC channel, parses the announcements, and forwards those announcements to ARR PVRs (eg Sonarr).</Description>
  <MyIP/>
  <DonateText/>
  <DonateLink/>
  <DonateImg/>
  <WebUI>http://[IP]:[PORT:7337]</WebUI>
  <ExtraParams/>
  <PostArgs/>
  <Network>bridge</Network>
  <Config Name="Host Port for 7337" Target="7337" Default="7337" Mode="tcp" Description="Container Port: 7337" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Host Path for /config" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="UMASK" Target="UMASK" Default="002" Mode="" Description="Container Variable: UMASK" Type="Variable" Display="always" Required="false" Mask="false"/>
</Container>
